.prizes{
    clear: both;
    max-width: 700px;
    margin: 0 auto;
    text-align: center
}

.prizes .item{
    display:    block;
    width:      305px;
}

.prizes .item .image{
    display:    block;
    clear:      both;
    padding:    0 0 15px 0;
    text-align: center;
}

.prizes .item .text{
    display:        block;
    clear:          both;
    font-weight:    bold;
    font-size:      17px;
    padding:        0 0 30px 0;
}

.prizes .item .morelink{
    display:    block;
    clear:      both;
    text-align: center;
    margin-bottom: 55px
}

.prizes .title {
    font-size: 24px;
    text-transform: uppercase;
    display: inline-block;
    text-align: center;
    margin-bottom: 30px;
    font-weight: bold;
    color: #44c0c9;
    border-bottom: 3px solid #44c0c9
}

p a{
  color: #44c0c9;
}

.only-desktop{
  display: block
}

.only-mobile{
  display: none
}

/* ------------------------------- */
/* CLEAN STYLE
/* ------------------------------- */
.cleanStyle
{
      line-height: 30px;
      padding: 100px 0 40px
}

.cleanStyle h1
{
      color: #44c0c9;
      text-transform: uppercase;
      border-bottom: 3px solid #44c0c9;
      display: inline-block;
      font-weight:  700;
      margin: 0;
      margin-bottom: 30px;
      font-family: Arial;
}

.cleanStyle h2
{
      font-size:    37px;
      font-weight:  400;
      color:        #000000;
      margin:       0 0 15px 0;
      font-family:  Arial;
      line-height:  42px;
}

.cleanStyle h3
{
      font-size:    27px;
      font-weight:  normal !important;
      color:        #656c72;
      margin:       0 0 12px 0;
      font-family: Arial;
}

.cleanStyle h4
{
      font-size:    17px;
      font-weight:  700 !important;
      color:        #656c72;
      margin:       0 0 12px 0;
      font-family: Arial;
}

.cleanStyle p
{
    font-weight: bold;
      margin: 0 0 20px 0;
      font-size: 17px
}

.cleanStyle strong
{
      font-weight: bold;
}

.cleanStyle em{
      font-style:   normal;
      font-size:    17px;
}

.cleanStyle u
{
      text-decoration: underline;
}

.cleanStyle strike
{
      text-decoration: line-through;
}

.cleanStyle ul
{
      width: 70%;
      display: block;
      margin: 5px 0 15px;
      list-style-position: inside;
      text-align: left;
      line-height: 20px;
}

.cleanStyle ul li
{
      display: list-item !important;
      margin: 10px 0 15px 0;
      font-size: 17px;
      font-weight: bold;
      padding: 0 !important;
}

.cleanStyle ul ul, .cleanStyle ul ul ul
{
      margin: 5px 0;
}

.cleanStyle ol
{
      display: block;
      margin: 5px 0 15px 0;
      padding: 0 0 0 15px;
      list-style-type: decimal;
      list-style-position: outside;
      clear: both;
      line-height: 20px;
}

.cleanStyle ol ol, .cleanStyle ol ol ol
{
      margin: 5px 0;
}

.cleanStyle ol li
{
      display: list-item !important;
      margin: 0 0 5px 0;
      padding: 0 !important;
}

.cleanStyle ol li
{
      list-style-image: none;
      list-style-type: decimal;
      list-style-position: outside;
}

.cleanStyle ol li li
{
      list-style-image: none;
      list-style-type: decimal;
      list-style-position: outside;
}

.cleanStyle ol li li li
{
      list-style-image: none;
      list-style-type: decimal;
      list-style-position: outside;
}

.cleanStyle blockquote
{
      margin-left: 40px;
}

.cleanStyle hr
{
      clear: both;
      margin: 5px 0 15px 0;
      border: 1px solid #6d7496;
}

.cleanStyle table
{
      width: 100%;
      margin: 5px 0 15px 0;
      border-right: 1px solid #959595;
      border-bottom: 1px solid #7f7f7f;
}

.cleanStyle table thead th
{
      color: #4b4b4b;
      padding: 5px;
      text-align: center;
      background: #dddcdc;
}

.cleanStyle table td
{
      border-top: 1px solid #7f7f7f;
      border-left: 1px solid #7f7f7f;
}
.cleanStyle table th
{
      border-top: 1px solid #959595;
      border-left: 1px solid #959595;
}

.cleanStyle table tbody th
{
      color: #4b4b4b;
      padding: 5px;
      text-align: center;
      background: #dddcdc;
}
.cleanStyle table tbody td
{
      color: #828282;
      padding: 5px 0;
      text-align: center;
      background: #ebeaea;
}

.cleanStyle table p
{
      margin: 0 !important;
}

.cleanStyle a
{
      color: #d71920;
}



@media screen and (max-width: 620px) {
  .general-content {
      background: url(../images/illustration-small.png) no-repeat center bottom;
      background: url(../images/illustration-small.png) no-repeat center bottom;
      background-size: 100%;
      padding-bottom: 300px;
  }
}


@media screen and (max-width: 768px) {

    .cleanStyle p,
    .cleanStyle ul li{
      font-size: 15px;
      line-height: 22px;
    }

    .cleanStyle ul{
      width: 90%;
    }

    .general-content{
      padding-bottom: 300px
    }

    .only-desktop{
      display: none
    }

    .only-mobile{
      display: block
    }

    .fb-Navigation.only-mobile{
      text-align: center;
    }

    .fb-Navigation.only-mobile a{
      margin: 20px 10px 0;
    }

    .fb-footer,
    .fb-push {
      padding: 45px 0 0px;
    }

    .fb-push img {
        display: inline-block;
        width: 15%;
        margin-right: 20px;
        vertical-align: middle;
    }

    .prizes .title{
      font-size: 24px;
    }

    .header{
      padding: 0;
    }

    .prizes .item .text {
        min-height: auto;
        margin-bottom: 20px;
    }

    .fb-head {
        height: auto;
    }

    .fb-head .bg-image {
        position: static;
    }

    .fb-head .bg-image .vouchers {
        background-position: 15% center
    }

    .fb-head .fb-head-logo {
        position: static;
        clear: both;
        width: 100%;
        height: auto
    }

    .fb-head .fb-head-logo:after {
        display: none;
    }

    .fb-head .fb-head-content {
        position: relative;
        width: auto;
        right: auto;
        text-align: center;
    }

    .fb-head .fb-head-content h1 {
        width: 80%;
        margin: 0 auto 30px;
        font-size: 26px;
        line-height: 31px;
    }

    .fb-head .fb-head-content p {
      width: 80%;
      margin: 0 auto 30px;
      font-size: 22px;
    }

    .fb-head .corner {
        top: 397px;
    }

    .mobile-lang .desktop {
        display: none;
    }

    .navigation-menu .main-Navigation {
        display: none;
    }

    .navigation-menu {
        text-align: center;
        width: auto;
        display: none!important
    }

    .navigation-menu .fb-Navigation {
        float: none;
        clear: both;
        margin: 0;
    }

    .navigation-menu .fb-Navigation a{
      margin-bottom: 30px;
    }

    .navigation-menu .fb-Navigation .fb-button {
        float: none!important;
        clear: both!important;
        display: inline-block;

    }

    .main-menu{
        display: none
    }

    .header .mobile{
        display: block;
    }

    .mobile .menu{
        border-bottom: 1px solid #a8a2ab;
        background: #fff;
    }

    .mobile .menu div{
        clear: none !important;
    }

    .mobile .menu ul{
        display: none;
        position: absolute !important;
        top: 68px;
        left: 0;
        width: 100% !important;
        z-index: 50
    }

    .mobile .menu ul li:first-child{
       border-top: 1px solid #a8a2ab;
    }

    .mobile .menu ul li{
       clear: both;
    }

     .mobile .menu ul li a{
       display: block;
       text-align: center;
       font-family: 'Lato', sans-serif;
       font-weight: 300;
       background: #fff;
       padding: 10px 0;
       text-decoration: none;
       width: 100%;
       color: #656c72;
       font-size: 27px;
       border-bottom: 1px solid #a8a2ab;
    }

    .mobile .menu ul li.fb a,
    .mobile .menu ul li.fb a:hover{
        background: #2e4586 url(../images/facebook-logo-mobile.png) no-repeat right center;
        color: #fff;
    }

    .mobile .menu ul li:hover a{
        background: #e5e5e5;
    }

    .mobile .menu ul li a:active{
        background: #e5e5e5
    }

    .mobile .menu-button{
        display: inline-block;
        width: 62px;
        height: 68px;
        background: url(../images/menu-mobile.png) no-repeat 0 0;
        background-size: 100%;
        border-right: 1px solid #d7d7d7;
        float: left;
    }

    .mobile .menu-button ul.show{
        display: block;
    }

    .mobile .logo-mobile{
        display: inline-block;
        min-width: 160px;
        margin: 0 10px;
        height: 68px;
        background: url(../images/tap-logo-mobile.png) no-repeat center center;
        background-size: 100%;
        float: left;
    }

    .mobile .mobile-lang{
        float: right;
        padding-top: 25px;
        height: 44px;
        margin-right: 10px
    }

    .mobile .mobile-lang span{
        color: #787f85;
        text-decoration: underline;
        font-size: 13px
    }

     .mobile .mobile-lang a{
        text-decoration: none;
        margin-right: 5px;
        font-size: 13px
    }

    .fb-footer .fb-Center {
        padding: 30px 0 0 0;
    }

    .fb-footer .partner,
    .fb-footer .victoria {
        float:      left;
        clear:      both;
        width:      100%;
        padding:    0 0 15px 0;
        margin: 0;
        text-align: center;
    }

    .fb-footer .partner img,
    .fb-footer .victoria img{
        display: inline-block;
    }


    .fb-footer, .fb-push {
        height: 75px;
    }

    .fb-Center{
        max-width:  300px;
        padding:    0 10px;
    }

    .prizes .item {
        float: none!important;
        clear: both!important;
        margin: 0 auto;
        padding: 0 0 50px 0;
        width: auto;
    }

    .dialog .buttons {
        text-align: center;
    }

    .dialog .buttons a {
        clear: both;
        margin: 0  0 20px 0;
    }
}
@media screen and (max-width: 360px){
  .prizes .title{
    font-size: 22px;
  }

  .general-content {
    padding-bottom: 200px;
  }
}

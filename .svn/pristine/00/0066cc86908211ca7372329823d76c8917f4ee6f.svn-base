
@font-face {
    font-family: 'flama';
    src: url('../fonts/flama-basic-webfont.eot');
    src: url('../fonts/flama-basic-webfont.eot?#iefix') format('embedded-opentype'),
         url('../fonts/flama-basic-webfont.ttf') format('truetype'),
         url('../fonts/flama-basic-webfont.svg#flamabasic') format('svg');
    font-weight: normal;
    font-style: normal;
}

@font-face {
    font-family: 'flama-bold';
    src: url('../fonts/ufonts.com_flama-bold-opentype-webfont.eot');
    src: url('../fonts/ufonts.com_flama-bold-opentype-webfont.eot?#iefix') format('embedded-opentype'),
         url('../fonts/ufonts.com_flama-bold-opentype-webfont.ttf') format('truetype'),
         url('../fonts/ufonts.com_flama-bold-opentype-webfont.svg#flamabold') format('svg');
    font-weight: normal;
    font-style: normal;

}


/* ------------------------------- */
/* HTML 5 ELEMENTS
/* ------------------------------- */
article, aside, figure, figcaption, footer, header, hgroup, menu, nav, section { display: block; }

/* ------------------------------- */
/* GENERAL ELEMENTS
/* ------------------------------- */
*{
      margin: 0;
      padding: 0;
}

a, a:active{
      cursor:           pointer;
      color:            #000000;
      text-decoration:  underline;
}
a:hover
{
      text-decoration: none;
}

img,
fieldset
{
      border: 0;
}

html,
body,
form
{
    min-height: 100%;
}

body{
    /*font:           100%/1.2 'Lato', sans-serif;*/
    font: 100%/1.2 'brandon', sans-serif;
    font-weight: 400;
    background: #caeaf1;

    color: #d24442;
}

.hidden{
    display: none;
}

.invisible{
    visibility: hidden;
}

/* dialog boxes */
.dialog-overlay {
    position:   absolute;
    z-index:    1001;
    display:    none;
    width:      100%;
    height:     100%;
    background: #caeaf1;

}

.close-overlay {
    clear:      both;
    text-align: center;
    height:     70px;
}

.close-overlay a{
    display:    inline-block;
    zoom:       1;
    *display:   inline;
    width:      17px;
    height:     17px;
    text-indent: 9999px;
    overflow:   hidden;
    background: url(../images/x.png) no-repeat left top;
}

.close-overlay a:hover {
    background: url(../images/x_hover.png) no-repeat left top;
}

.title-overlay{
    width:      100%;
    clear:      both;
    text-align: center;
    padding:    0 0 15px;
}

.title-overlay .title{
    width:      100%;
    clear:      both;
    font-size:  20px;
    color:      #ffffff;
    padding:    0 0 15px;
}

.title-overlay .subtitle{
    width:      100%;
    clear:      both;
    font-size:  12px;
    color:      #a0b1e1;
    padding:    0 0 15px;
}

.title-overlay .link{
    width:      100%;
    clear:      both;
    font-size:  12px;
    padding:    0 0 15px;
}

.title-overlay .link a {
    color:              #a0b2e2;
    text-decoration:    none;
}

.title-overlay .link a:hover {
    color:              #ffffff;
    text-decoration:    underline;
}

.dialog
{
    position:       absolute;
    top:            0;
    z-index:        50;
    display:        none;
    width:          100%;
    padding:        120px 0 0;
    text-align: center;
}

.dialog .buttons{
    text-align: center;
}

.dialog h2{
    font-weight: 300;
    text-align: center;

    font-size: 22px;
    display: inline-block;
    margin: 0 auto 20px;
    color: #44c0c9;
    border-bottom: 3px solid #44c0c9;
    font-weight: bold;
    text-transform: uppercase;
}

.dialog p{
    line-height: 22px;
    font-weight: 300;
    text-align: center;
    font-size: 20px;
    margin-bottom: 40px;
    font-size: 17px;
    font-weight: bold
}

.dialog ul{
    margin-left: 20px;
    font-size: 15px
}

.dialog ul li
{
    line-height: 22px
}

.dialog .fb-Center
{
    max-width: 480px;
}

.dialog .buttons{
    padding:    20px 0 0 0;
    text-align: center;
}

.dialog .buttons-simple
{
    padding: 20px 0 0 0;
    background: none;
}

.dialog .orlabel
{
    display: block;
    clear: both;
    text-align: center;
    font-size: 17px;
    color: #ffffff;
    padding: 20px 0 0 0;
    text-transform: lowercase;
}

.dialog .share-on-fb{
    width:      100%;
    padding:    30px 0 0 0;
}

/* clearfix propertie */
.clearfix {
    zoom: 1; /* For IE 6/7 (trigger hasLayout) */
}

.clearfix:after {
  content:  "";
  display:  table;
  clear:    both;
}

.red-button {
    cursor: pointer;
    display: inline-block;
    color: #caeaf1;
    font-size: 11px;
    padding: 12px 15px;
    text-decoration: none;
    background: #d24442;
    text-transform: uppercase;
    font-weight: bold
}

.red-button:hover{
    background: #c4403e;
}

.red-button:active{
    text-decoration: none;
    color: #fff
}

.blue-button {
    cursor: pointer;
    position: relative;
    display: inline-block;
    color: #fff;
    font-size: 11px;
    padding: 12px 15px;
    text-decoration: none;
    background: #6c82d1;
    text-transform: uppercase;
    font-weight: bold;
    border-right: 1px solid #fff;
    margin-right: 57px!important
}

.blue-button:after{
  content: '';
  height: 37px;
  width: 37px;
  display: block;
  position: absolute;
  background-color: #6c82d1;
  background-image: url(../images/facebook-logo.png);
  right: -38px;
  top: 0;
}

.blue-button:hover:after,
.blue-button:hover{
    background-color: #6072b6;
}

.blue-button:active{
    text-decoration: none;
    color: #fff
}

.icon-heart{
  display: inline-block;
  position: relative;
  top: 1px;
  height: 10px;
  width: 10px;
  background: url(../images/icon-heart.png) no-repeat 0 0
}

.red-button.has-icon{
  padding-left: 10px;
  padding-right: 10px;
}

.red-button .icon-heart:first-of-type{
  margin-right: 5px
}

.red-button .icon-heart:last-of-type{
  margin-left: 5px
}

.goLeft, .pull-left{
    float: left;
    clear: left!important;
}

.goRight, .pull-right{
    float: right;
    clear: right!important;
}


/* ------------------------------- */
/* TAP HEADER
/* ------------------------------- */

.header
{
    background: #e9ebf2;
    padding: 30px 10px;
}

.header .mobile
{
    display: none
}

.header .main-menu
{
    background: #FFF;
    padding-top: 10px;
    max-width:  986px;
    margin: 0 auto;
    text-align: center;
    border-radius: 100px;
    text-align: center
}

.header .main-menu .header-title
{
    padding: 13px 10px 7px 6px;
    background: url(../images/tap-lettering.png) no-repeat 0 0;
    color: #000;
    width: 200px;
    display: inline-block;
}

.header .main-menu a
{
    text-decoration: none;
    color: #666;
    font-size: 12px;
    margin-left: 20px;
    padding: 15px 0;
}

.header .main-menu a:hover
{
    color: #000
}

.header .main-menu a span
{
    margin: 0 20px;
    border-bottom: 2px solid #F00;
    padding-bottom: 14px;
}

.header .main-menu a.pull-left
{
    background: url(../images/arrow-mainmenu-left.gif) no-repeat left 12px
}

.header .main-menu a.en
{
    background: url(../images/en.gif) no-repeat left 14px;
    padding-left: 15px
}

.header .main-menu a.pt
{
    background: url(../images/pt.gif) no-repeat left 14px;
    padding-left: 15px
}

.fb-head  img{
  max-width: 1005px;
  width: 90%
}

/* ------------------------------- */
/* PAGE STRUCTURE
/* ------------------------------- */
.fb-Center {
    max-width:  768px;
    margin:     0 auto;
    clear:      both;
}

.fb-Wrapper {
    width:          100%;
    clear:          both;
    min-height:     100%;
    height:         auto!important;
    height:         100%;
    padding:        0;
    margin:         0 auto -121px;
}

.fb-head {
  padding-top: 30px;
  text-align: center;
  width:          100%;
  clear:          both;
}

.fb-head .fb-head-logo {
    display:    block;
    clear:      both;
}

.fb-head-content {
    width: 410px;
    text-align: center;
    padding: 80px 0 90px;
    margin: 0 auto

}


.fb-head .fb-head-logo h1 {
   display: none
}

.fb-head .fb-head-logo p{

    font-size: 26px;
    font-weight: bold;
}

.fb-head .fb-head-logo-alt {
    display:        block;
    clear:          both;
    text-align:     center;
    font-family:    'brandon_grotesqueregular';
    font-size:      23px;
    color:          #ff6464;
    padding:        10px 0 0 0;
}

.navigation-menu{
    display:        block;
    clear:          both;
    width: 450px
}

.main-Navigation{
    float:          left;
    font-size:      19px;
    font-family:    'brandon_grotesqueblack';
    text-transform: uppercase;
 }

.main-Navigation ul {
    display:    inline-block;
    clear:      both;
    list-style: none;
}

.main-Navigation ul li {
    float: left;
}

.main-Navigation ul li a {
    display:                block;
    line-height:            40px;
    color:                  #ffffff;
    padding:                0 15px;
    text-decoration:        none;
    outline:                none;
    -webkit-border-radius:  10px;
    -moz-border-radius:     10px;
    border-radius:          10px;
    background:             #e30610;
}

.main-Navigation ul li a:hover,
.main-Navigation ul li a.selected {
    background: #c3060f;
}

.fb-Navigation{
  text-align: right;
  max-width: 1024px;
  width: 90%;
  margin: 0 auto 15px;
}

.fb-Navigation a{
  margin-right: 20px
}

.navigation-menu .fb-Navigation {
    float:          left;
    font-size:      19px;
    font-family:    'brandon_grotesqueblack';
    text-transform: uppercase;
}

.navigation-menu .fb-Navigation .fb-button {
    display:                block;
    line-height:            40px;
    color:                  #ffffff;
    padding:                0 15px;
    text-decoration:        none;
    outline:                none;
    -webkit-border-radius:  8px;
    -moz-border-radius:     8px;
    border-radius:          8px;
    background:             #ff6464;
}

.navigation-menu .fb-Navigation .fb-button:hover {
    background: #3ceecd;

}


.general-content {
    position: relative;
    clear: both;
    width: 100%;
    padding-bottom: 500px;
    background: url(../images/illustration-large.png) no-repeat center bottom;
    border-bottom: 4px solid #d44543;
}


.fb-content {
    position:   relative;
    float:      left;
    clear:      both;
    width:      100%;
    padding:    50px 0 115px 0;
    background: #f4f4f4;
}

/* footer */
.fb-footer, .fb-push {
    clear:  both;
    padding: 45px 0 50px;
	   height: 31px; /* .push must be the same height as .footer */
     background: #ebeaf1
}

.fb-push{
  text-align: center;
}

.fb-push img{
  display: inline-block;
  margin-right: 40px
}

